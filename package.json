{
  "name": "wedding-inv-go-server",
  "version": "1.0.0",
  "description": "Server to handle wedding inv go requests",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node .",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-spain/wedding-inv-go-server.git"
  },
  "author": "Sam Spain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sam-spain/wedding-inv-go-server/issues"
  },
  "homepage": "https://github.com/sam-spain/wedding-inv-go-server#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^6.0.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.4",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.1.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.1.6",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  }
}
